    "node-html-parser": "^6.1.13",
    "nodemailer": "^6.9.13",
    "nodemailer-sendinblue-transport": "^2.0.1",
    "playwright": "^1.43.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20"
